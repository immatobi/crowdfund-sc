{
  "name": "crowdfund",
  "version": "1.0.0",
  "description": "web3 crowdfund project",
  "main": "server.js",
  "directories": {
    "test": "./tests"
  },
  "scripts": {
    "start": "NODE_ENV=production NODE_OPTIONS='--max_old_space_size=19000' ts-node --respawn --transpile-only src/server.ts",
    "dev": "NODE_OPTIONS='--max_old_space_size=18000' ts-node-dev --respawn --transpile-only src/server.ts",
    "seed-d": "ts-node-dev remover.ts -d",
    "pretest": "NODE_ENV=test npm run seed-d",
    "test": "NODE_ENV=test mocha",
    "compile-c": "node src/_dapp/compile.js",
    "deploy-c": "ts-node-dev src/_dapp/deploy.ts",
    "tc": "truffle compile",
    "tm": "truffle migrate --network goerli"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "mocha": {
    "spec": "./tests",
    "recursive": true,
    "diff": true,
    "extension": [
      "js",
      "ts"
    ],
    "timeout": 30000,
    "slow": 1500,
    "ui": "bdd",
    "reporter": "spec"
  },
  "keywords": [
    "crowdfund"
  ],
  "author": "Immanuel",
  "license": "MIT",
